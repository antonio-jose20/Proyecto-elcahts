
@{
    ViewBag.Title = "Modificar";
}

<!DOCTYPE html>
<html ng-app="App">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="~/Angular/angular.js"></script>
    <script src="~/Angular/Usuariosclientes.js"></script>
</head>
<body ng-controller="UsuariosClientes">

    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="jumbotron jumbotron-fluid">
                    <div class="container">
                        <h1 class="display-4">Modificar</h1>
                        <p class="lead">Descripcion.</p>
                        <button type="button" class="btn btn-outline-danger" ng-click="irIndex()">Regresar</button>
                    </div>
                </div>
            </div>

        </div>


        <form class="row">
            <div class="col-md-12">
                <h3 class="text-center">Modificar Usuarios </h3>
            </div>
            <div class="col-md-12 form-group">
                <label>Nombre usuario</label>
                <input ng-model="NombreCompleto" value="@Model.NombreCompleto" class="form-control" />
            </div>
            <div class="col-md-12 form-group">
                <label>Apellidos</label>
                <input ng-model="Apellidos"  value="@Model.Apellidos"class="form-control" />
            </div>
            <div class="col-md-12 form-group">  
                <label>Direccion</label>
                <input ng-model="Direccion" value="@Model.Direccion" class="form-control" />
            </div>
            <div class="col-md-12 form-group">
                <label>Dui</label>
                <input ng-model="Dui"  Direccionclass="form-control" />
            </div>
            <div class="col-md-12 form-group">
                <label>Telefono</label>
                <input ng-model="Telefono"  value="@Model.Dui" class="form-control" />
            </div>
            <div class="col-md-12 form-group">
                <label>Email</label>
                <input ng-model="Email" value="@Model.Email" class="form-control" />
            </div>
            <div class="col-md-12 form-group">
                <label>Password</label>
                <input ng-model="Password" value="@Model.Password" class="form-control" />
            </div>
            <div class="col-md-12 form-group">
                <p class="text-right">
                    <button class="btn btn-primary" ng-click="ModificarUsuariosclientes()">Guardar</button>
                </p>
                <br />
                <br />
                <br />
            </div>

        </form>




    </div>



    <div class="container body-content">
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
</body>
</html>

