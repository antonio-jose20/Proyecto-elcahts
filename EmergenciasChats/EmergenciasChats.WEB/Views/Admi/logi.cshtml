@model EmergenciasChats.EL.AdminEL

@{
    ViewBag.Title = "logi";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>logi</h2>
<div id="contenido">
    <div class="principal">
        <img src="~/Image/foto2.jpg" alt="shopping" width="40" height="50" />
    </div>

    <div class="aside">
        <h2>Iniciar Sesión</h2>
        <form method="post" action="@Url.Action("logi", "Admi")">
            <div class="form-group">
                <label>Email</label>
                <input type="email" name="Email" class="form-control" />
            </div>
            <div class="form-group">
                <label>Contraseña</label>
                <input type="password" name="Password" class="form-control" />
            </div>
            @*<a href="@Url.Action("Create", "Admin")"  class="btn btn-info btn-block">Ingresar</a>*@
            <button type="submit" class="btn btn-info btn-block">Ingresar</button>
        </form>

        <hr />
        <!--<p>No tienes una cuenta?. <a href=" Url.Action("Registro", "Cliente")">Registrarse</a></p>-->
        <p>No tienes una cuenta?. <a href="@Url.Action("Create", "Admi")">Registrarse</a></p>

    </div>
</div>

